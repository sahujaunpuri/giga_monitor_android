<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:top="6dp"
        android:right="6dp"
        android:left="6dp"
        android:bottom="6dp">
        <shape>
            <solid android:color="#fcf6d6" android:width="5px"/>

            <stroke android:width="6dp" android:color="#ffffff" />

            <size  android:height="6dp" />

            <corners android:radius="15dp" />
        </shape>
    </item>
    <item android:top="4dp"
        android:right="4dp"
        android:left="4dp"
        android:bottom="4dp">
        <shape>
            <stroke android:color="#c49d26" android:width="2dp" />

            <size android:height="7dp" />

            <corners android:radius="15dp" />
        </shape>
    </item>
</layer-list>